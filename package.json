{
  "name": "native-datetime",
  "version": "0.1.0",
  "description": "Date and Time picker for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/YogendraShelke/native-datetime",
  "author": "Yogendra Shelke <yogendra.s@somaiya.edu> (https://github.com/YogendraShelke)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YogendraShelke/native-datetime/issues"
  },
  "homepage": "https://github.com/YogendraShelke/native-datetime#readme",
  "devDependencies": {
    "@react-native-community/bob": "^0.16.2",
    "react": "16.11.0",
    "react-native": "0.62.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "moment": "^2.22.0",
    "@react-native-community/datetimepicker": "^2.4.0"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  }
}
